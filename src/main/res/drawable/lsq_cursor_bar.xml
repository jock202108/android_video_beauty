<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="@color/lsq_color_transparent"/>
            <size android:height="67dp" android:width="3dp"/>
        </shape>

    </item>
    <item android:left="1dp" android:right="1dp">

        <shape
            xmlns:android="http://schemas.android.com/apk/res/android"
            >

            <corners android:radius="1dp"></corners>
            <solid android:color="@color/lsq_color_white" ></solid>
            <size android:width="1dp" android:height="67dp"/>
        </shape>

    </item>



</layer-list>
