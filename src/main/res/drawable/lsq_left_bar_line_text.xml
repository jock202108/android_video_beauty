<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>

        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <corners android:bottomLeftRadius="8dp" android:topLeftRadius="8dp"></corners>
            <solid android:color="#FF1010"/>
            <size android:height="70dp" android:width="11dp"/>
        </shape>

    </item>

    <item  android:gravity="center" >
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <corners android:radius="4dp"></corners>
            <solid android:color="@color/lsq_color_white"/>
            <size android:height="16dp" android:width="2dp"/>
        </shape>
    </item>

</layer-list>
