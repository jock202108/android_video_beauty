<?xml version="1.0" encoding="utf-8"?>
<org.lasque.twsdkvideo.video_beauty.views.editor.playview.TuSdkRangeSelectionBar
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lsq_range_select"
    android:layout_width="match_parent"
    android:clickable="true"
    android:layout_height="57dp">

    <org.lasque.twsdkvideo.video_beauty.views.editor.playview.rangeselect.TuSdkMovieGrayView
        android:id="@+id/lsq_left_bar"
        android:layout_width="14dp"
        android:layout_height="match_parent"
        >
        <View
            android:id="@+id/lsq_left_bar_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="14dp"
            android:background="@drawable/lsq_left_bar_rect"
            />
        <View
            android:id="@+id/lsq_left_bar_line"
            android:layout_width="14dp"
            android:layout_height="match_parent"
            android:layout_gravity="right"
            android:background="@drawable/lsq_left_bar_line"
            />
<!--        <ImageView-->
<!--            android:layout_width="11dp"-->
<!--            android:layout_height="match_parent"-->
<!--            android:src="@drawable/edit_ic_arrow"-->
<!--            android:padding="4dp"-->
<!--            android:background="@drawable/lsq_left_bar"-->
<!--            android:layout_gravity="right"-->
<!--            />-->
    </org.lasque.twsdkvideo.video_beauty.views.editor.playview.rangeselect.TuSdkMovieGrayView>

    <FrameLayout
        android:id="@+id/lsq_center"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_toLeftOf="@+id/lsq_right_bar"
        android:layout_toRightOf="@+id/lsq_left_bar">

        <View
            android:id="@+id/lsq_center_top_line"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_gravity="top"
            android:background="@color/lsq_color_white" />

        <View
            android:id="@+id/lsq_center_cover"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:background="#66FF1010"
            android:visibility="gone" />

        <View
            android:id="@+id/lsq_center_bottom_line"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:layout_gravity="bottom"
            android:background="@color/lsq_color_white" />
    </FrameLayout>

    <org.lasque.twsdkvideo.video_beauty.views.editor.playview.rangeselect.TuSdkMovieGrayView
        android:id="@+id/lsq_right_bar"
        android:layout_width="14dp"
        android:layout_height="match_parent"
        android:layout_alignParentRight="true"
        >

        <View
            android:id="@+id/lsq_right_bar_bg"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="14dp"
            android:background="@drawable/lsq_right_bar_rect"
            />

        <View
            android:id="@+id/lsq_right_bar_line"
            android:layout_width="14dp"
            android:layout_height="match_parent"
            android:layout_gravity="left"
            android:background="@drawable/lsq_right_bar_line"
            />
    </org.lasque.twsdkvideo.video_beauty.views.editor.playview.rangeselect.TuSdkMovieGrayView>

</org.lasque.twsdkvideo.video_beauty.views.editor.playview.TuSdkRangeSelectionBar>