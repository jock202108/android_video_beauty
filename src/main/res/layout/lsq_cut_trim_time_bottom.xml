<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_alignParentBottom="true"
    android:background="@drawable/trim_bottom_bg"
    android:id="@+id/item_layout"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/lsq_range_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="8dp"
            android:layout_toLeftOf="@+id/centerTv"
            android:paddingTop="28dp"
            android:text=""
            android:textSize="11sp"
            android:textColor="@color/cardview_light_background" />

        <TextView
            android:id="@+id/centerTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:paddingTop="28dp"
            android:text="/"
            android:textColor="@color/secondary_text"
            android:textSize="11sp"/>

        <TextView
            android:id="@+id/lsq_total_time"
            android:layout_toRightOf="@+id/centerTv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="8dp"
            android:paddingTop="28dp"
            android:text=""
            android:textColor="@color/secondary_text"
            android:textSize="11sp"/>
    </RelativeLayout>


    <org.lasque.twsdkvideo.video_beauty.views.editor.playview.TuSdkMovieScrollContent
        android:id="@+id/lsq_range_line"
        android:layout_width="match_parent"
        android:layout_height="99dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="15dp"
        android:layout_marginRight="18dp"
        android:layout_marginBottom="30dp"
        app:coverListHeight="72dp"
        app:coverListTopMargin="13.5dp"
        app:cursorHeight="99dp" />



    <include layout="@layout/lsq_editor_component_home_bottom"></include>


</LinearLayout>