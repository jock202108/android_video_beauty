<?xml version="1.0" encoding="utf-8"?>
<org.lasque.twsdkvideo.video_beauty.views.ConfigViewSeekBar xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >




    <RelativeLayout
        android:id="@+id/lsq_titleWrapView"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true" >

        <TextView
            android:id="@+id/lsq_titleView"
            android:layout_width="80dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="18dp"
            android:layout_centerVertical="true"
            android:ellipsize="end"
            android:gravity="start"
            android:lines="1"
            android:text="@string/lsq_filter_set_mixied"
            android:textColor="@color/lsq_seekbar_drag_color"
            android:textSize="11sp" />

    </RelativeLayout>


    <include
        android:id="@+id/lsq_seekView"
        layout="@layout/widget_seekbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginRight="20dp"
        android:layout_toRightOf="@+id/lsq_titleWrapView" />
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:layout_toRightOf="@+id/lsq_titleWrapView"
        android:layout_marginBottom="3dp"
        android:layout_above="@+id/lsq_seekView"
        >
        <TextView
            android:id="@+id/lsq_configValueView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:lines="1"
            android:text=""
            android:textColor="@color/lsq_seekbar_drag_color"
            android:textSize="11sp"/>
    </RelativeLayout>



</org.lasque.twsdkvideo.video_beauty.views.ConfigViewSeekBar>

